{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "target": "es2017",
    "noImplicitAny": true,
    "allowJs": true,
    "isolatedModules": false,
    "moduleResolution": "node",
    "types": ["vitest/globals"],
    "emitDeclarationOnly": true
  },
  "exclude": ["node_modules", "dist"],
  "include": ["vitest.config.ts", "solve"]
}
